        <PAGEOBJECT XPOS="140" YPOS="1404.5" OwnPage="3" ItemID="1821604389" PTYPE="4" WIDTH="160" HEIGHT="199" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PCOLOR2="Black" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="0" AUTOTEXT="0" EXTRA="3.6" TEXTRA="3.6" BEXTRA="3.6" REXTRA="3.6" VAlign="0" FLOP="1" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L160 0 L160 199 L0 199 L0 0 Z" copath="M0 0 L160 0 L160 199 L0 199 L0 0 Z" gXpos="140" gYpos="1404.5" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="-1" BACKITEM="-1">
            <StoryText>
                <DefaultStyle/>
                <ITEXT FONT="DejaVu Sans Bold" CH="Checks and saves"/>
                <para/>
                <ITEXT FONTSIZE="8" CH="As a player, the keeper may ask you to make checks or saves to decide the outcome of an action. A "/>
                <ITEXT FONT="DejaVu Sans Bold" FONTSIZE="8" CH="check"/>
                <ITEXT FONTSIZE="8" CH=" is made to resolve your character's action, whereas a "/>
                <ITEXT FONT="DejaVu Sans Bold" FONTSIZE="8" CH="save"/>
                <ITEXT FONTSIZE="8" CH=" is made to resolve your character's reaction to something in the world. In either case, this entails rolling a D20 and adding any relevant modifiers, bonuses/penalties, or bonus/penalty dice. The sum of the roll is compared with a target number called a difficulty class("/>
                <ITEXT FONT="DejaVu Sans Bold" FONTSIZE="8" CH="DC"/>
                <ITEXT FONTSIZE="8" CH=") or an Armor Class("/>
                <ITEXT FONT="DejaVu Sans Bold" FONTSIZE="8" CH="AC"/>
                <ITEXT FONTSIZE="8" CH=") in the case of making an attack. If your roll meets or beats the target number, your check or save is successful."/>
                <trail/>
            </StoryText>
        </PAGEOBJECT>
        <PAGEOBJECT XPOS="300.5" YPOS="1404.5" OwnPage="3" ItemID="1821820709" PTYPE="4" WIDTH="155" HEIGHT="119" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PCOLOR2="Black" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="0" AUTOTEXT="0" EXTRA="3.6" TEXTRA="3.6" BEXTRA="3.6" REXTRA="3.6" VAlign="0" FLOP="1" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L155 0 L155 119 L0 119 L0 0 Z" copath="M0 0 L155 0 L155 119 L0 119 L0 0 Z" gXpos="300.5" gYpos="1404.5" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="-1" BACKITEM="-1">
            <StoryText>
                <DefaultStyle/>
                <ITEXT FONT="DejaVu Sans Bold" FONTSIZE="12" CH="Ability scores and modifiers"/>
                <para ALIGN="0"/>
                <ITEXT FONTSIZE="8" CH="An "/>
                <ITEXT FONT="DejaVu Sans Bold" FONTSIZE="8" CH="ability score"/>
                <ITEXT FONTSIZE="8" CH=" is a number from 1-20 which represents one  of a character's abilities and is used to calculate the modifier applied to said character's ability checks. The "/>
                <ITEXT FONT="DejaVu Sans Bold" FONTSIZE="8" CH="ability modifier"/>
                <ITEXT FONTSIZE="8" CH=" for each ability score is shown in the table below."/>
                <trail/>
            </StoryText>
        </PAGEOBJECT>

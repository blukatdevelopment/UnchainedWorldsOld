        <PAGEOBJECT XPOS="1015.8008" YPOS="4469.9648" OwnPage="10" ItemID="1930832490" PTYPE="4" WIDTH="274.0224" HEIGHT="659.052799999999" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PCOLOR2="Black" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="0" AUTOTEXT="0" EXTRA="3.6" TEXTRA="3.6" BEXTRA="3.6" REXTRA="3.6" VAlign="0" FLOP="1" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L274.022 0 L274.022 659.053 L0 659.053 L0 0 Z" copath="M0 0 L274.022 0 L274.022 659.053 L0 659.053 L0 0 Z" gXpos="1015.8008" gYpos="6233.7488" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="-1" BACKITEM="-1">
            <StoryText>
                <DefaultStyle/>
                <ITEXT CH="List of Skills"/>
                <para PARENT="H2"/>
                <ITEXT CH="Sleight Of Hand"/>
                <para PARENT="H3"/>
                <ITEXT CH="Successfully pickpocketing, disarming traps, picking locks."/>
                <para/>
                <para/>
                <ITEXT CH="Acrobatics"/>
                <para PARENT="H3"/>
                <ITEXT CH="Performing stunts with agility and precision, swinging from chandliers, rolling to avoid harm from a fall, running across a tight-rope, leaping great distances, etc."/>
                <para/>
                <para/>
                <ITEXT CH="Athletics"/>
                <para PARENT="H3"/>
                <ITEXT CH="Performing stunts with brute strength, breaking doors down, climbing, etc."/>
                <para/>
                <para/>
                <ITEXT CH="Stealth"/>
                <para PARENT="H3"/>
                <ITEXT CH="Moving quietly, staying hidden, and avoiding detection."/>
                <para/>
                <para/>
                <ITEXT CH="Pathfinding"/>
                <para PARENT="H3"/>
                <ITEXT CH="Navigating by the stars, tracking creatures, and locating shortcuts."/>
                <para/>
                <para/>
                <ITEXT CH="Persuasion"/>
                <para PARENT="H3"/>
                <ITEXT CH="Appealing to a subject's self-interest, rationality, morality, or other traits tactfully in order to change their mind"/>
                <para/>
                <para/>
                <ITEXT CH="Deception"/>
                <para PARENT="H3"/>
                <ITEXT CH="Bluffing, exaggerating, implying untruths, denying truths, or otherwise fabricating details convincingly"/>
                <para/>
                <para/>
                <ITEXT CH="Intimidation"/>
                <para PARENT="H3"/>
                <ITEXT CH="Creating a threatening presence in order to gain undue influence over someone through fear, on or off the battlefield"/>
                <para/>
                <para/>
                <ITEXT CH="Insight"/>
                <para PARENT="H3"/>
                <ITEXT CH="Determining someone's general disposition, current mood, stress levels, and detecting deception"/>
                <para/>
                <para/>
                <ITEXT CH="Foraging"/>
                <para PARENT="H3"/>
                <ITEXT FONT="DejaVu Sans Bold" CH=" "/>
                <ITEXT CH="Locating and identifying edible plants, fungus, fish for food, medicine or alchemy."/>
                <para/>
                <para/>
                <ITEXT CH="History"/>
                <para PARENT="H3"/>
                <ITEXT CH="Recalling the details of famous people, places, objects, or cultures through secondary sources such as conversing with others or reading texts."/>
                <para/>
                <para/>
                <ITEXT CH="Religion"/>
                <para PARENT="H3"/>
                <ITEXT CH="Recall the lore of divines, undead, fiends, fey, and extra-planar beings. Pray to, contact, bargain with, summon, or detect these same entities."/>
                <para/>
                <para/>
                <ITEXT CH="Performance"/>
                <para PARENT="H3"/>
                <ITEXT CH="Evoke feelings such as wonder, joy, rage, serenity, or sorrow through a performing art. Arts include, but are not limited to music, poetry, public speaking, dancing, storytelling, acting, and flirting."/>
                <para/>
                <para/>
                <ITEXT CH="Crafting"/>
                <para PARENT="H3"/>
                <ITEXT CH="Create, repair, or enhance items."/>
                <para/>
                <para/>
                <ITEXT CH="Alchemy"/>
                <para PARENT="H3"/>
                <ITEXT CH="Brew an ingredient to produce a tincture with one effect, or brew multiple inctures together to create a potion with some mixture or synthesis of their effects."/>
                <para/>
                <para/>
                <ITEXT CH="Medicine"/>
                <para PARENT="H3"/>
                <ITEXT CH="Perform field medicine on a dying or injured ally, or create a healer's kit from foraged ingredients. "/>
                <trail/>
            </StoryText>
        </PAGEOBJECT>
        <PAGEOBJECT XPOS="100.5008" YPOS="7075.6376" OwnPage="15" ItemID="1932397706" PTYPE="4" WIDTH="594.2744" HEIGHT="19.0048000000002" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PCOLOR2="Black" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="0" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="1" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L594.274 0 L594.274 19.0048 L0 19.0048 L0 0 Z" copath="M0 0 L594.274 0 L594.274 19.0048 L0 19.0048 L0 0 Z" gXpos="695.7752" gYpos="1821.96160000001" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="-1" BACKITEM="-1">

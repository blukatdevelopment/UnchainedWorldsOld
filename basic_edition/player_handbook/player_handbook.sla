                <ITEXT CH=": Dexterity"/>
                <para/>
                <ITEXT FONT="DejaVu Sans Bold" CH="CON"/>
                <ITEXT CH=": Constitution"/>
                <para/>
                <ITEXT FONT="DejaVu Sans Bold" CH="INT"/>
                <ITEXT CH=": Intelligence"/>
                <para/>
                <ITEXT FONT="DejaVu Sans Bold" CH="WIS"/>
                <ITEXT CH=": Wisdom"/>
                <para/>
                <ITEXT FONT="DejaVu Sans Bold" CH="CHA"/>
                <ITEXT CH=": Charisma"/>
                <para/>
                <ITEXT FONT="DejaVu Sans Bold" CH="Skill die"/>
                <ITEXT CH=": A die you may add as a bonus to checks for a particular skill."/>

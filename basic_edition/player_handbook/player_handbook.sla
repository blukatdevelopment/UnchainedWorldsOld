        <PAGEOBJECT XPOS="875.7864" YPOS="1784.2856" OwnPage="4" ItemID="1976846826" PTYPE="4" WIDTH="179.0176" HEIGHT="379.0304" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PCOLOR2="Black" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="0" AUTOTEXT="0" EXTRA="3.6" TEXTRA="3.6" BEXTRA="3.6" REXTRA="3.6" VAlign="0" FLOP="1" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L179.018 0 L179.018 379.03 L0 379.03 L0 0 Z" copath="M0 0 L179.018 0 L179.018 379.03 L0 379.03 L0 0 Z" gXpos="679.0608" gYpos="2666.1776" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="-1" BACKITEM="-1">
            <StoryText>
                <DefaultStyle/>
                <ITEXT CH="Adventuring Gear"/>
                <para PARENT="H2"/>
                <para/>
                <ITEXT CH="Large Items"/>
                <para PARENT="H3"/>
                <ITEXT CH="10 gp - (10 slots)Canoe"/>

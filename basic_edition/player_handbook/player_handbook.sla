                <para/>
                <ITEXT FONT="DejaVu Sans Bold" CH="Bonus:"/>
                <ITEXT CH=" A die or number you add to a roll"/>
                <para/>
                <ITEXT FONT="DejaVu Sans Bold" CH="Penalty"/>
                <ITEXT CH=": A die or number you subtract from a roll"/>
                <para/>
                <ITEXT FONT="DejaVu Sans Bold" CH="HP"/>

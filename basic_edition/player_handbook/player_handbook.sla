        <PAGEOBJECT XPOS="100.5008" YPOS="2706.1808" OwnPage="5" ItemID="1963480442" PTYPE="4" WIDTH="259.0208" HEIGHT="59.0047999999997" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PCOLOR2="Black" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="0" AUTOTEXT="0" EXTRA="3.6" TEXTRA="3.6" BEXTRA="3.6" REXTRA="3.6" VAlign="0" FLOP="1" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L259.021 0 L259.021 59.0048 L0 59.0048 L0 0 Z" copath="M0 0 L259.021 0 L259.021 59.0048 L0 59.0048 L0 0 Z" gXpos="100.5008" gYpos="2706.1808" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="-1" BACKITEM="-1">

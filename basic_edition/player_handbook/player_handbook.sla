                <ITEXT CH="- Sleeping on a dry bedroll"/>
                <para/>
                <ITEXT CH="- Eating a meal(Small item's worth of food)"/>
                <para/>
                <ITEXT CH="- Eating a second serving"/>
                <para/>
                <ITEXT CH="- Consuming a vice(alcohol, caffeine, tobacco, sweets)"/>
                <trail/>
            </StoryText>
        </PAGEOBJECT>
        <PAGEOBJECT XPOS="696.5" YPOS="3996.99473684211" OwnPage="12" ItemID="1855326309" PTYPE="4" WIDTH="155" HEIGHT="174.50526315789" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PCOLOR2="Black" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="0" AUTOTEXT="0" EXTRA="3.6" TEXTRA="3.6" BEXTRA="3.6" REXTRA="3.6" VAlign="0" FLOP="1" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L155 0 L155 174.505 L0 174.505 L0 0 Z" copath="M0 0 L155 0 L155 174.505 L0 174.505 L0 0 Z" gXpos="696.5" gYpos="3996.99473684211" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="-1" BACKITEM="-1">

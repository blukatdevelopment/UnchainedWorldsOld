            <StoryText>
                <DefaultStyle/>
                <ITEXT FONT="DejaVu Sans Bold" CH="Perk: Defense"/>
                <para/>
                <ITEXT CH="Each time you select this perk, roll 2d6 for a benefit."/>
                <para/>
                <ITEXT FONT="DejaVu Sans Bold" CH="2. "/>
                <ITEXT CH=" Once per rest, spend 10 minutes to restore D4 HP"/>
                <para/>
                <ITEXT FONT="DejaVu Sans Bold" CH="3. "/>
                <ITEXT CH="Once per rest gain D6 THP that lasts 1 hour"/>
                <para/>
                <ITEXT FONT="DejaVu Sans Bold" CH="4-8."/>
                <ITEXT CH=" Gain +1 to max HP"/>
                <para/>
                <ITEXT FONT="DejaVu Sans Bold" CH="9-10."/>
                <ITEXT CH=" +2 to STR, DEX, or CON, or +1 to two"/>
                <para/>
                <ITEXT FONT="DejaVu Sans Bold" CH="11."/>
                <ITEXT CH=" Once per rest, take half damage for D6 turns"/>
                <para/>
                <ITEXT FONT="DejaVu Sans Bold" CH="12."/>
                <ITEXT FONT="DejaVu Sans Book" CH=" +1 to AC when wearing heavy armor"/>
                <trail/>
            </StoryText>
        </PAGEOBJECT>
        <PAGEOBJECT XPOS="536" YPOS="2216.5" OwnPage="6" ItemID="671280561" PTYPE="4" WIDTH="159.5" HEIGHT="79" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PCOLOR2="Black" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="0" AUTOTEXT="0" EXTRA="3.6" TEXTRA="3.6" BEXTRA="3.6" REXTRA="3.6" VAlign="0" FLOP="1" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L159.5 0 L159.5 79 L0 79 L0 0 Z" copath="M0 0 L159.5 0 L159.5 79 L0 79 L0 0 Z" gXpos="536" gYpos="2216.5" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="-1" BACKITEM="-1">

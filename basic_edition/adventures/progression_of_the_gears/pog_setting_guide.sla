            <StoryText>
                <DefaultStyle/>
                <ITEXT CH="Common Steamlands Gear"/>
                <para PARENT="H2"/>
                <para PARENT="H2"/>
                <ITEXT CH="Chain Knife(25GP)"/>
                <para PARENT="H3"/>
                <ITEXT CH="Small Sly, D4 damage. On hit, use additional actions to autohit for more damage. Uses steam.*"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Musket Pistol(25GP)"/>
                <para PARENT="H3"/>
                <ITEXT CH="Small Ranged, 60ft range, 3d4 DMG, 3 action reload."/>
                <para/>
                <ITEXT CH="Musket(50GP)"/>
                <para PARENT="H3"/>
                <ITEXT CH="Large Ranged, 60ft range, 3d6 DMG, 3 action reload."/>
                <para/>
                <ITEXT CH="Power Drill(100GP)"/>
                <para PARENT="H3"/>
                <ITEXT CH="Large martial. On hit, use additional actions to autohit for more damage."/>
                <para/>
                <ITEXT CH="Auto-crossbow(150GP)"/>
                <para PARENT="H3"/>
                <ITEXT CH="Extra large(2 slots) ranged, 120ft range, Deals D12 damage. Uses steam.*"/>
                <para/>
                <ITEXT CH="Chainsaw(200GP). Uses steam.*"/>
                <para PARENT="H3"/>
